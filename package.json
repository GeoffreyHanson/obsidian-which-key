{
	"name": "which-key-obsidian",
	"version": "0.9.0-beta",
	"description": "Popup menu for displaying keybindings on the fly, bringing the functionality known to emacs and vim users to Obsidian.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"test": "jest",
		"test:watch": "jest --watch",
		"lint": "eslint 'src/**/*.ts' --fix",
		"lint:check": "eslint 'src/**/*.ts'",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"which-key",
		"keybindings",
		"shortcuts",
		"menu",
		"vim",
		"emacs"
	],
	"author": "Geoffrey Hanson",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/geoffreyhanson/obsidian-which-key.git"
	},
	"bugs": {
		"url": "https://github.com/geoffreyhanson/obsidian-which-key/issues"
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^20.17.30",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.25.2",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-prettier": "^5.2.3",
		"jest": "^29.7.0",
		"obsidian": "latest",
		"prettier": "^3.5.3",
		"ts-jest": "^29.3.1",
		"ts-node": "^10.9.2",
		"tslib": "2.4.0",
		"typescript": "^4.7.4"
	}
}
